{
  "name": "thermal-printer-typescript",
  "version": "1.0.0",
  "description": "TypeScript project to print on thermal printer via COM port on macOS",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "serialport": "^10.5.0",
    "sharp": "^0.33.4",
    "usb": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.101",
    "@types/serialport": "^8.0.2",
    "@types/usb": "^2.0.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.4"
  }
}
